generator client {
  provider = "prisma-client-js"
  output   = "../generated/prisma"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

enum TypeOfTask {
  BASH
  DOCKER
  NODE
}

model Task {
  id          Int        @id @default(autoincrement())
  command     String
  scheduledAt Int
  typeOfTask  TypeOfTask
}
